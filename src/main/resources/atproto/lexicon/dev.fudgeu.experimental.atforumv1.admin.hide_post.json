        "type": "object",
        "required": ["target", "createdAt"],
        "properties": {
          "target": {
            "type": "string",
            "format": "at-uri"
          },
          "createdAt": {
            "type": "string",
            "format": "datetime"
          }
        }
      }
    }
  }
}